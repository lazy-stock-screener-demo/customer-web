{
  "name": "stock-web",
  "version": "0.0.1",
  "description": "Stock Web Servie in Daniel's US Stock Screener",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:lazy-stock-screener/stock-web.git"
  },
  "keywords": [
    "react",
    "typescript",
    "webpack",
    "graphql"
  ],
  "main": "index.js",
  "scripts": {
    "dev:run-client": "NODE_ENV=development npx webpack-dev-server --config ./config/client/webpack.js --color --progress --hot",
    "dev:run-client-bundle-analyze": "BUNDLE_ANALYZE=analyze npm run dev:run-client",
    "dev:run-client-mock-server": "nodemon --watch mock --exec 'json-server ./mock/_db.js --config ./mock/config.json' --signal SIGHUP ./mock/server.js",
    "dev:run-client-mock": "npm run dev:run-client-mock-server & npm run dev:run-client",
    "prod:build-client": "NODE_ENV=production webpack --config ./config/client/webpack.js",
    "prod:build-client-bundle-analyze": "BUNDLE_ANALYZE=analyze npm run prod:build-client",
    "prod:build-server": "NODE_ENV=production webpack --config ./config/server/webpack.js",
    "prod:build-all": "npm run prod:build-client && npm run prod:build-server",
    "prod:build-and-run-server-host-static": "npm run prod:build-all && node ./build-server/server.js",
    "prod:run-server-host-static": "node ./build-server/server.js"
  },
  "author": "Daniel Lin",
  "license": "(Apache-2.0)",
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@hookform/error-message": "0.0.5",
    "@loadable/component": "^5.13.1",
    "@loadable/server": "^5.13.1",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@types/d3-scale": "^3.2.2",
    "axios": "^0.21.1",
    "bufferutil": "^4.0.1",
    "clsx": "^1.1.1",
    "cors": "^2.8.5",
    "d3": "^6.5.0",
    "d3-shape": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "jest": "^26.1.0",
    "joi": "^17.3.0",
    "match-sorter": "^6.3.0",
    "path": "^0.12.7",
    "react": "^16.13.1",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^6.14.1",
    "react-loadable": "^5.5.0",
    "react-loading-overlay": "^1.0.1",
    "react-redux": "^7.2.2",
    "react-router-config": "^5.1.1",
